{
  "main": "index.ts",
  "scripts": {
    "dev": "pm2 kill && pm2 start index.sh --watch && pm2 log",
    "dev:win": "ts-node-dev -r tsconfig-paths/register -r dotenv-expand/config --files index.ts --watch",
    "db:sync": "ts-node -r tsconfig-paths/register -r dotenv-expand/config --files db-sync.ts",
    "build": "ts-node -r tsconfig-paths/register -r dotenv-expand/config --files build/index.ts",
    "start": "ts-node -r tsconfig-paths/register -r dotenv-expand/config --files index.ts",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.5",
    "@types/sequelize": "^4.28.15",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.1.4",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.3",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.0",
    "sharp": "^0.32.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0"
  }
}
